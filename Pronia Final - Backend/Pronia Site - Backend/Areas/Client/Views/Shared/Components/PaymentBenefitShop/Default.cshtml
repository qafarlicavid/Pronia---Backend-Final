@using Pronia_Site___Backend.Areas.Client.ViewModels.Home.Index
@model IndexViewModel

@foreach (var paymentBenefits in Model.PaymentBenefits)
{
    <li class="service-item">
        <div class="service-img">
            <img src="~/@paymentBenefits.BackGroundImageUrl"
             alt="Shipping Icon" />
        </div>
        <div class="service-content">
            <span class="title">
                @paymentBenefits.Title 
                <br />
            </span>
        </div>
    </li>
}