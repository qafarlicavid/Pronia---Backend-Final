@using Pronia_Site___Backend.Areas.Client.ViewModels.Benefit
@model List<BenefitListItemViewModel>

<div class="row shipping-wrap">
    @foreach (var benefit in Model)
    {
            
    <div class="col-lg-4 col-md-6">
        <div class="shipping-item">
            <div class="shipping-img">
                <img src="~/@benefit.ImageUrl"
                     alt="Shipping Icon" />
            </div>
            <div class="shipping-content">
                <h2 class="name">@benefit.Name</h2>
                <p class="short-desc mb-0">@benefit.Content</p>
            </div>
        </div>
    </div>
    }

</div>